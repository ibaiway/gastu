{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "esModuleInterop": true,
        "target": "ES2022",
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
        "removeComments": true,
        "forceConsistentCasingInFileNames": true,
    },
    "include": ["src/**/*"]
}